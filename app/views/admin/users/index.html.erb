<h3>ユーザー一覧</h3>
<table>
  <thead>
    <tr>
      <th>ユーザーID</th>
      <th>ユーザー名</th>
      <th>メールアドレス</th>
      <th>会員ステータス</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td>
        <%= link_to admin_user_path(user) do %>
          <%= user.name %>
        <% end %>
      </td>
      <td><%= user.email %></td>
      <td><%= user.is_active %></td>
      <td><%= link_to '変更', edit_admin_user_path(user) %></td>
    </tr>
    <% end %>
  </tbody>
</table>